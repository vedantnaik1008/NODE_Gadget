    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "rootDir": "./",
        "outDir": "./dist",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true
    },
    "include": ["**/*.ts"],
    "exclude": ["dist"]
}
